{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$ref": "#/definitions/IBotCore",
	"definitions": {
		"IBotCore": {
			"type": "object",
			"properties": {
				"emoji": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/IEmoji"
					}
				},
				"colors": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/IColor"
					}
				},
				"id": {
					"type": "string"
				},
				"objectType": {
					"type": "string",
					"const": "Bot"
				},
				"did": {
					"$ref": "#/definitions/Snowflake"
				},
				"codeName": {
					"$ref": "#/definitions/TBotCodeName"
				},
				"commandPrefix": {
					"type": "string"
				},
				"devs": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/TDev"
					}
				},
				"token": {
					"type": "string"
				},
				"tokenUrl": {
					"type": "string"
				},
				"searchStatus": {
					"type": "object",
					"additionalProperties": {
						"anyOf": [
							{
								"not": {}
							},
							{
								"type": "boolean"
							},
							{
								"type": "string"
							}
						]
					}
				}
			},
			"required": [
				"codeName",
				"colors",
				"did",
				"id",
				"objectType",
				"searchStatus",
				"token",
				"tokenUrl"
			],
			"additionalProperties": false
		},
		"IEmoji": {
			"type": "object",
			"properties": {
				"type": {
					"$ref": "#/definitions/EmojiType"
				},
				"matches": {
					"type": "array",
					"items": {
						"type": "string"
					}
				},
				"replacement": {
					"type": "string"
				}
			},
			"required": [
				"type",
				"matches",
				"replacement"
			],
			"additionalProperties": false
		},
		"EmojiType": {
			"type": "number",
			"enum": [
				11,
				12,
				13,
				14,
				15,
				16,
				21,
				22,
				31,
				32,
				33,
				34,
				35,
				4,
				41,
				42,
				43,
				44,
				45,
				461,
				4611,
				4612,
				462,
				4621,
				4622,
				463,
				4631,
				4632,
				464,
				4641,
				4642,
				465,
				4651,
				4652,
				51,
				52,
				53,
				54,
				61,
				62,
				70,
				701,
				702,
				71,
				711,
				8100,
				8101,
				8102,
				8103,
				8104,
				8105,
				8106,
				8107,
				8108,
				90
			]
		},
		"IColor": {
			"type": "object",
			"properties": {
				"type": {
					"$ref": "#/definitions/ColorType"
				},
				"hex": {
					"type": "string"
				},
				"label": {
					"type": "string"
				}
			},
			"required": [
				"type",
				"hex"
			],
			"additionalProperties": false
		},
		"ColorType": {
			"type": "number",
			"enum": [
				1,
				2,
				3,
				31,
				4,
				5,
				51,
				52,
				521,
				522,
				523,
				524,
				53,
				531,
				532,
				533,
				534,
				6
			]
		},
		"Snowflake": {
			"type": "string"
		},
		"TBotCodeName": {
			"type": "string",
			"enum": [
				"dev",
				"beta",
				"stable"
			]
		},
		"TDev": {
			"type": "object",
			"properties": {
				"did": {
					"$ref": "#/definitions/Snowflake"
				}
			},
			"required": [
				"did"
			],
			"additionalProperties": false
		}
	}
}